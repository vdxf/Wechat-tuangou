{
  "pages": [
    {
      "path": "pages/home/home",
      "style": {
        "navigationBarBackgroundColor": "#FD3423",
        "navigationBarTitleText": "新友团"
      }
    }, 
    {
      "path": "pages/search/search",
      "style": {
        "navigationBarTitleText": "搜索",
        "enablePullDownRefresh": false,
        "navigationBarBackgroundColor": "#f5f5f5",
        "usingComponents": {
          "van-search": "/wxcomponents/vant/weapp/search/index",
          "van-calendar": "/wxcomponents/vant/weapp/calendar/index"
        }
      }
    }
  ],
  "subPackages": [
    {
      "root": "subpackage",
      "pages": [
        {
          "path": "goods_detail/goods_detail",
          "style": {
            "navigationBarTitleText": "团品详情",
            "navigationStyle": "custom"
          }
        }
      ]
    }
  ],
  "globalStyle": {
    "navigationBarTextStyle": "black",
    "navigationBarBackgroundColor": "#fff",
    "backgroundColor": "#fff",
    "app-plus": {
      "background": "#efeff4"
    }
  },
  "tabBar": {
    "selectedColor": "#FA2D19",
    "list": [
      {
        "pagePath": "pages/home/home",
        "text": "首页",
        "iconPath": "static/image/home.png",
        "selectedIconPath": "static/image/home-active.jpg"
      },
      {
        "pagePath": "pages/search/search",
        "text": "搜索",
        "iconPath": "static/image/search.jpg",
        "selectedIconPath": "static/image/search-active.png"
      }
    ]
  }
}