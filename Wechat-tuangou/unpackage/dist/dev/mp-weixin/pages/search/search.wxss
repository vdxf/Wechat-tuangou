/* 水平间距 */
/* 水平间距 */
page {
  background: linear-gradient(#f5f5f5 0%, rgba(245, 245, 245, 0) 100%);
}
.search-box {
  background-color: #f5f5f5;
  padding-right: 20rpx;
  position: -webkit-sticky;
  position: sticky;
  top: 0;
  z-index: 2;
}
.van-search__content {
  border: 1px solid #ccc;
  position: relative;
  height: 72rpx;
  padding-right: 150rpx;
}
.search-btn {
  position: absolute;
  right: 26rpx;
  top: 26rpx;
  z-index: 2;
  background-color: #F0190F;
  color: #fff;
  height: 64rpx;
  width: 128rpx;
  text-align: center;
  border-radius: 32rpx;
}
.search-result {
  height: 40rpx;
  font-size: 24rpx;
  color: #999999;
  line-height: 40rpx;
  padding: 0 40rpx;
  margin-bottom: 20rpx;
}

